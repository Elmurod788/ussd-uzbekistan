<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ucellLogin">
    <TextView
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Ucell"
            android:textColor="#0022E1"
            android:textSize="30dp"
            android:layout_marginTop="35dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.498"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="75dp"
        android:layout_marginBottom="0dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <Button
            android:id="@+id/btn_callCenter_Ucell"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_marginStart="0dp"
            android:onClick="startPhoneActionIntent"
            android:background="@drawable/ic_call" />

        <TextView
            android:id="@+id/txt_callCenter_Ucell"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/btn_callCenter_Ucell"
            android:layout_alignEnd="@+id/btn_callCenter_Ucell"
            android:layout_alignParentStart="true"
            android:layout_marginTop="5dp"
            android:text="Call Center"
            app:layout_constraintEnd_toEndOf="@+id/btn_callCenter_Ucell"
            app:layout_constraintStart_toStartOf="@+id/btn_callCenter_Ucell"
            app:layout_constraintTop_toBottomOf="@+id/btn_callCenter_Ucell" />

        <Button
            android:id="@+id/btn_main"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/ic_home"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/txt_main"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/btn_main"
            android:layout_alignEnd="@+id/btn_main"
            android:layout_alignParentStart="true"
            android:layout_marginTop="5dp"
            android:text="Main"
            app:layout_constraintEnd_toEndOf="@+id/btn_main"
            app:layout_constraintHorizontal_bias="0.491"
            app:layout_constraintStart_toStartOf="@+id/btn_main"
            app:layout_constraintTop_toBottomOf="@+id/btn_main" />

        <Button
            android:id="@+id/btn_cabinet_Ucell"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/ic_cabinet"
            app:layout_constraintEnd_toEndOf="parent" />

        <TextView
            android:id="@+id/txt_cabinet_Ucell"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/btn_cabinet_Ucell"
            android:layout_alignEnd="@+id/btn_cabinet_Ucell"
            android:layout_alignParentStart="true"

            android:text="Cabinet"
            app:layout_constraintEnd_toEndOf="@+id/btn_cabinet_Ucell"
            app:layout_constraintHorizontal_bias="0.491"
            app:layout_constraintStart_toStartOf="@+id/btn_cabinet_Ucell"
            app:layout_constraintTop_toBottomOf="@id/btn_cabinet_Ucell" />

    </androidx.constraintlayout.widget.ConstraintLayout>


</androidx.constraintlayout.widget.ConstraintLayout>
